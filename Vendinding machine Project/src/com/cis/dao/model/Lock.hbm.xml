<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cis.dao.model.Lock1"  table="Lock1">
         <id name="lockNo" type="java.lang.Integer">
            <column name="lockNo" />
             <generator class="increment" /> 
        </id> 
        <property name="lockID" type="java.lang.String">
            <column name="lockID" />
        </property>
       
        <property name="machineNAME" type="java.lang.String">
            <column name="machineNAME" />
        </property>
           <property name="machineSIZE" type="java.lang.String">
            <column name="machineSIZE" />
        </property>
       
        <property name="machinePrice" type="java.lang.String">
            <column name="machinePrice" />
        </property>
       
       <property name="file" type="binary">
            <column name="file_data" />
        </property>
    </class>
</hibernate-mapping>